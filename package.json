{
  "name": "Delicious-Shop-server",
  "version": "1.0.0",
  "description": "Delicious-Shop",
  "engines": {
    "node": "8.9.x"
  },
  "scripts": {
    "start": "node server",
    "heroku-postbuild": "cd react-ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "client": "cd react-ui && npm start",
    "server": "nodemon server/index",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "express": "^4.16.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HollyBang/Delicious-Shop.git"
  },
  "keywords": [
    "node",
    "heroku",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^3.6.1",
    "nodemon": "^1.18.3"
  }
}
